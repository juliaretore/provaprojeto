<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.prova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Prova</name>
    <description>
        Template para prova de projeto
    </description>
    <author email="wellirigo@gmail.com" href="#">
        Kennedy Ara√∫jo
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="res/icon.png" />
    
    <plugin name="cordova-plugin-vibration" spec="~3.1.0" />
    <engine name="android" spec="^7.0.0" />
</widget>
